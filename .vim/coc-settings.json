{
	"extensions.updateCheck": "weekly",

	"coc.source.file.ignoreHidden": false,
	"diagnostic.checkCurrentLine": true,

	"go.checkForUpdates": "disabled",
	"go.goplsPath": "$HOME/go/bin/gopls",

	"rust-analyzer.server.path": "$HOME/.cargo/bin/rust-analyzer",
	"workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],

	"@nomicfoundation/coc-solidity.telemetry": false
}
